{
  "name": "gitcoin-personal-token-contracts",
  "version": "1.0.0",
  "description": "Smart contracts for Gitcoin personal tokens functionality",
  "main": "index.js",
  "scripts": {
    "compile": "npx buidler compile",
    "test": "npx buidler test",
    "deploy-dev": "npm run compile && npx buidler run --network rinkeby scripts/deploy-factory.js",
    "deploy-prod": "npm run compile && npx buidler run --network mainnet scripts/deploy-factory.js",
    "lint": "npx solhint **/*.sol",
    "prettier": "prettier --write **/*.sol"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raid-guild/gitcoinco-ptoken-contracts.git"
  },
  "license": "AGPLv3",
  "bugs": {
    "url": "https://github.com/raid-guild/gitcoinco-ptoken-contracts/issues"
  },
  "homepage": "https://github.com/raid-guild/gitcoinco-ptoken-contracts#readme",
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.3",
    "@nomiclabs/buidler-ethers": "^1.3.3",
    "@nomiclabs/buidler-etherscan": "^2.1.0",
    "@nomiclabs/buidler-waffle": "^1.3.3",
    "@openzeppelin/contracts": "^3.0.1",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^2.5.0",
    "ethers": "^4.0.47",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.55",
    "solhint": "^3.1.0"
  }
